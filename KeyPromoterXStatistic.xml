<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Close">
        <Item>
          <IdeaActionID>CloseContent</IdeaActionID>
          <ShortCut>'Ctrl+F4'</ShortCut>
          <Description>Close</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Commit Message History">
        <Item>
          <IdeaActionID>Vcs.ShowMessageHistory</IdeaActionID>
          <ShortCut>'Ctrl+M' or 'Ctrl+E'</ShortCut>
          <Description>Commit Message History</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Commit...">
        <Item>
          <IdeaActionID>CheckinProject</IdeaActionID>
          <ShortCut>'Ctrl+K'</ShortCut>
          <Description>Commit...</Description>
          <Count>15</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug">
        <Item>
          <IdeaActionID>ActivateDebugToolWindow</IdeaActionID>
          <ShortCut>'Alt+5'</ShortCut>
          <Description>Debug</Description>
          <Count>7</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'build &amp; restart'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'Shift+F9'</ShortCut>
          <Description>Debug 'build &amp; restart'</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'restart-admin'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'Shift+F9'</ShortCut>
          <Description>Debug 'restart-admin'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Disconnect">
        <Item>
          <IdeaActionID>DatabaseView.CloseConnectionAction</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Disconnect</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Evaluate Expression...">
        <Item>
          <IdeaActionID>EvaluateExpression</IdeaActionID>
          <ShortCut>'Alt+F8'</ShortCut>
          <Description>Evaluate Expression...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find">
        <Item>
          <IdeaActionID>ActivateFindToolWindow</IdeaActionID>
          <ShortCut>'Alt+3'</ShortCut>
          <Description>Find</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="GoTo">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>GoTo</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Jump to Console...">
        <Item>
          <IdeaActionID>Jdbc.OpenConsole.Any</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F10'</ShortCut>
          <Description>Jump to Console...</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Messages">
        <Item>
          <IdeaActionID>ActivateMessagesToolWindow</IdeaActionID>
          <ShortCut>'Alt+0'</ShortCut>
          <Description>Messages</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Move...">
        <Item>
          <IdeaActionID>Move</IdeaActionID>
          <ShortCut>'F6'</ShortCut>
          <Description>Move...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'build &amp; restart'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Rerun 'build &amp; restart'</Description>
          <Count>20</Count>
        </Item>
      </Statistic>
      <Statistic Action="Resume Program">
        <Item>
          <IdeaActionID>Resume</IdeaActionID>
          <ShortCut>'F9'</ShortCut>
          <Description>Resume Program</Description>
          <Count>16</Count>
        </Item>
      </Statistic>
      <Statistic Action="Revert...">
        <Item>
          <IdeaActionID>ChangesView.Revert</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Z'</ShortCut>
          <Description>Revert...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Reword...">
        <Item>
          <IdeaActionID>Git.Reword.Commit</IdeaActionID>
          <ShortCut>'F2' or 'Shift+F6'</ShortCut>
          <Description>Reword...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run">
        <Item>
          <IdeaActionID>ActivateRunToolWindow</IdeaActionID>
          <ShortCut>'Alt+4'</ShortCut>
          <Description>Run</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>28</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'Alt+9' or 'Alt+Shift+9'</ShortCut>
          <Description>Version Control</Description>
          <Count>30</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>