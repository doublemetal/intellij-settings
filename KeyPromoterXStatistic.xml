<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Commit...">
        <Item>
          <IdeaActionID>CheckinProject</IdeaActionID>
          <ShortCut>'Ctrl+K'</ShortCut>
          <Description>Commit...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'build &amp; restart'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'Shift+F9'</ShortCut>
          <Description>Debug 'build &amp; restart'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Disconnect">
        <Item>
          <IdeaActionID>DatabaseView.CloseConnectionAction</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Disconnect</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="GoTo">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>GoTo</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Jump to Console...">
        <Item>
          <IdeaActionID>Jdbc.OpenConsole.Any</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F10'</ShortCut>
          <Description>Jump to Console...</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Revert...">
        <Item>
          <IdeaActionID>ChangesView.Revert</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Z'</ShortCut>
          <Description>Revert...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run">
        <Item>
          <IdeaActionID>ActivateRunToolWindow</IdeaActionID>
          <ShortCut>'Alt+4'</ShortCut>
          <Description>Run</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'Alt+9' or 'Alt+Shift+9'</ShortCut>
          <Description>Version Control</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>